{
  "name": "pdf_to_text",
  "description": "A pipeline that converts a PDF to text by first extracting images and then running OCR.",
  "execution_mode": "simple",
  "pipeline": [
    {
      "name": "pdf_extraction_processor",
      "params": {
        "resolution": 300,
        "image_format": "PNG"
      }
    },
    {
      "name": "ocr_processor",
      "params": {
        "language": "eng"
      }
    }
  ]
}