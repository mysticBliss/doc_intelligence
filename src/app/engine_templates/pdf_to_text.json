{
  "name": "pdf_to_text",
  "description": "A pipeline that converts a PDF to text by first extracting images and then running OCR.",
  "execution_mode": "simple",
  "pipeline": [
    {
      "id": "extract_images",
      "name": "pdf_extraction_processor",
      "params": {
        "resolution": 300,
        "image_format": "PNG"
      }
    },
    {
      "id": "run_ocr",
      "name": "ocr_processor",
      "dependencies": ["extract_images"],
      "params": {
        "language": "eng"
      }
    }
  ]
}