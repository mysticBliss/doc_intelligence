{
  "name": "image_deskew_ocr",
  "description": "A simple workflow that first deskews an image and then runs OCR.",
  "execution_mode": "simple",
  "pipeline": [
    {
      "id": "deskew_image",
      "name": "image_preprocessing_processor",
      "params": {
        "steps": [
          {
            "name": "deskew"
          }
        ]
      }
    },
    {
      "id": "run_ocr",
      "name": "ocr_processor",
      "dependencies": ["deskew_image"],
      "params": {
        "language": "eng"
      }
    }
  ]
}